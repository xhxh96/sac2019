{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-standard",
    "tslint-react"
  ],
  "rules": {
    "prettier": true,
    "ordered-imports": false,
    "object-literal-shorthand": false,
    "member-ordering": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}